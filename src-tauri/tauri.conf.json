{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "c-tools",
  "version": "0.1.0",
  "identifier": "Cyd",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "cTools",
        "width": 800,
        "height": 200,
        "label": "main",
        "minimizable": true,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "publisher": "Cyd",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/json.ico",
      "icons/sql.ico",
      "icons/文件树.ico",
      "icons/系统信息.ico",
      "icons/文件整理.ico",
      "icons/哈希.ico",
      "icons/正则.ico"
    ],
    "resources": [
      "icons/32x32.png"
    ]
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    },
    "api": {
      "all": true
    }
  }
}